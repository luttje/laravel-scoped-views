{
    "name": "luttje/laravel-scoped-views",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "description": "Scope CSS and JS in Laravel, using <x-scoped>components</x-scoped>.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luttje",
            "email": "2738114+luttje@users.noreply.github.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Luttje\\ScopedViews\\": "src/php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luttje\\ScopedViews\\ServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.4|^9.0",
        "orchestra/testbench-dusk": "^7.5"
    }
}
